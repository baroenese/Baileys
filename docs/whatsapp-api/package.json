{
  "name": "baileys-minimal-stable",
  "version": "1.0.0",
  "description": "Minimal and stable Baileys WhatsApp implementation focused on reliability and performance",
  "type": "module",
  "private": true,
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsx src/index.ts",
    "dev:watch": "tsx watch src/index.ts",
    "dev:debug": "tsx --inspect src/index.ts",
    "worker": "tsx src/worker.ts",
    "lint": "tsc --noEmit",
    "clean": "node -e \"require('fs').rmSync('dist',{recursive:true,force:true});require('fs').rmSync('logs',{recursive:true,force:true})\"",
    "typecheck": "tsc --noEmit",
    "prestart": "npm run build"
  },
  "keywords": [
    "whatsapp",
    "baileys",
    "whatsapp-web",
    "whatsapp-api",
    "signal-protocol",
    "e2e-encryption"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@cacheable/node-cache": "~1.4.0",
    "@hapi/boom": "~10.0.1",
    "@whiskeysockets/baileys": "file:../../",
    "bullmq": "^5.66.4",
    "dotenv": "^16.4.5",
    "ioredis": "^5.3.2",
    "pino": "~9.5.0",
    "pino-pretty": "~13.0.0"
  },
  "devDependencies": {
    "@types/node": "~22.10.0",
    "@types/qrcode-terminal": "^0.12.2",
    "qrcode-terminal": "^0.12.0",
    "tsx": "~4.19.0",
    "typescript": "~5.7.2"
  },
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=8.0.0"
  }
}
